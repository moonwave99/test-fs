{
  "name": "@moonwave99/test-fs",
  "description": "A tiny filesystem helper test library.",
  "repository": "https://github.com/moonwave99/test-fs",
  "homepage": "https://moonwave99.github.io/test-fs",
  "author": {
    "name": "Diego Caponera",
    "email": "hello@diegocaponera.com",
    "url": "https://www.diegocaponera.com"
  },
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint --ext .ts src",
    "test": "vitest run",
    "test:watch": "vitest --maxWorkers=1",
    "test:coverage": "vitest run --coverage --maxWorkers=1"
  },
  "keywords": [
    "fs",
    "test",
    "fixtures",
    "file system"
  ],
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "eslint": "^9.35.0",
    "fs-extra": "^11.3.1",
    "globals": "^16.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
